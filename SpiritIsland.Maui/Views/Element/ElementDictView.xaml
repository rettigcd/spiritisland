<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:si="clr-namespace:SpiritIsland.Maui"
			 x:Class="SpiritIsland.Maui.ElementDictView">

	<CollectionView ItemsSource="{Binding Elements}" ItemsLayout="HorizontalList" >
		<CollectionView.ItemTemplate>
			<DataTemplate x:DataType="si:ElementModel" >
				<si:ElementView HeightRequest="15" />
			</DataTemplate>
		</CollectionView.ItemTemplate>
	</CollectionView>
	
</ContentView>
